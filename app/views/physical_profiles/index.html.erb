<h1>Listing physical_profiles</h1>

<table>
  <thead>
    <tr>
      <th>Cpf</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @physical_profiles.each do |physical_profile| %>
      <tr>
        <td><%= physical_profile.cpf %></td>
        <td><%= link_to 'Show', physical_profile %></td>
        <td><%= link_to 'Edit', edit_physical_profile_path(physical_profile) %></td>
        <td><%= link_to 'Destroy', physical_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Physical profile', new_physical_profile_path %>
